{
  "version": 2,
  "builds": [
    { "src": "api/socket.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api/socket", "dest": "/api/socket.js" },
    { "src": "/socket.io/(.*)", "dest": "/api/socket.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 